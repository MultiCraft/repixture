msgid "Hunger Debug:"
msgstr "تصحيح الجوع:"

msgid "<hunger disabled>"
msgstr "<تم إيقاف تفعيل الجوع>"

msgid "You are hungry."
msgstr "أنت تشعر بالجوع."

msgid "You are starving."
msgstr "أنت تشعر بالجوع الشديد."

msgid "You starved to death."
msgstr "لقد جعت حتى الموت."

msgid "Eating"
msgstr "أكل"

msgid "You're eating food, which slows you down"
msgstr "أنت تأكل لذا ستكون أبطأ"

msgid "Gourmet"
msgstr "ذواق"

msgid "Eat everything that can be eaten."
msgstr "قم بأكل كل شيء يمكن أكله."

msgid "Set hunger level of player or yourself"
msgstr "حدد مستوى جوع لعب أو نفسك"

msgid "[<player>] <hunger>"
msgstr "[<لاعب>] <جوع>"

msgid "Player is not online."
msgstr "الاعب غير متصل."

msgid "No player."
msgstr "لا لاعب."

msgid "Food item"
msgstr "طعام"

msgid "Food points: +@1"
msgstr "نقاط الطعام: +@1"

msgid "Saturation points: +@1"
msgstr "نقاط التشبع: +@1"
