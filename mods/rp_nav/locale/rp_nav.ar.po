#: mods/rp_nav/compass.lua:523
msgid "True Navigator"
msgstr "ملاح حقيقي"

#: mods/rp_nav/compass.lua:524
msgid "Magnetize a compass."
msgstr "مغنط بوصلة."

#: mods/rp_nav/map.lua:68
msgid "Map"
msgstr "خريطة"

